{% raw %}
[program:{{ project_name }}]
command={{ env_path }}/{{ project_name }}/bin/gunicorn --workers 3 --bind 127.0.0.1:{{ port }} {{ wsgi_module }}:application
directory={{ project_path }}/{{ project_name }}
autostart=true
autorestart=true
stderr_logfile=/var/log/gunicorn/{{ project_name }}.err.log
stdout_logfile=/var/log/gunicorn/{{ project_name }}.out.log
{% endraw %}
